@prefix : <http://okfn.gr/ontology/nsrf-gr#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix vfp: <http://okfn.gr/ontology/vfp#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix nsrf-gr: <http://okfn.gr/ontology/nsrf-gr#> .
@base <http://okfn.gr/ontology/nsrf-gr> .

<http://okfn.gr/ontology/nsrf-gr> rdf:type owl:Ontology ;
                                   owl:versionIRI <http://okfn.gr/ontology/nsrf-gr/0.0.2> ;
                                   owl:imports <http://okfn.gr/ontology/vfp/0.0.2> ;
                                   <http://purl.org/dc/terms/creator> "Evangelos Chondrokostas"@en ;
                                   <http://purl.org/dc/elements/1.1/description> "Extension of VFP ontology for describing the projects of the Greek National Stratigic Reference Framework"@en ;
                                   owl:versionInfo "Version 0.0.2"@en ;
                                   <http://purl.org/dc/elements/1.1/title> "National Stratigic Reference Framework Greece vocabulary"@en .


#################################################################
#    Object Properties
#################################################################

###  http://okfn.gr/ontology/nsrf-gr#body
nsrf-gr:body rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain vfp:Project ;
             rdfs:range vfp:Organization ;
             rdfs:comment "Beneficiary of the NSRF project"@en ,
                          "Δικαιούχος του έργου ΕΣΠΑ"@el ;
             rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
             rdfs:label "Body-beneficiary"@en ,
                        "Δικαιούχος"@el .


###  http://okfn.gr/ontology/nsrf-gr#bodyType
nsrf-gr:bodyType rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain nsrf-gr:Body ;
                 rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
                 rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
                 rdfs:label "Body type"@en ,
                            "Είδος φορέα"@el .


###  http://okfn.gr/ontology/nsrf-gr#decisionAbout
nsrf-gr:decisionAbout rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      owl:inverseOf nsrf-gr:hasDecision ;
                      rdfs:domain nsrf-gr:DecisionData ;
                      rdfs:range nsrf-gr:Expropriation ;
                      rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
                      rdfs:label "decision about"@en ,
                                 "Απόφαση σχετικά"@el .


###  http://okfn.gr/ontology/nsrf-gr#exproOf
nsrf-gr:exproOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                owl:inverseOf nsrf-gr:hasExpro ;
                rdfs:comment "Beneficiary of the NSRF project"@en ,
                             "Δικαιούχος του έργου ΕΣΠΑ"@el ;
                rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
                rdfs:label "Body-beneficiary"@en ,
                           "Δικαιούχος"@el .


###  http://okfn.gr/ontology/nsrf-gr#exproURL
nsrf-gr:exproURL rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf vfp:url ,
                                    owl:topObjectProperty ;
                 rdfs:domain nsrf-gr:Expropriation ;
                 rdfs:range nsrf-gr:Document ;
                 rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
                 rdfs:label "Exrpopriation URL"@en ,
                            "URL απαλλοτρίωσης"@el .


###  http://okfn.gr/ontology/nsrf-gr#fileType
nsrf-gr:fileType rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain nsrf-gr:Document ;
                 rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
                 rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
                 rdfs:label "File Type"@en ,
                            "Είδος eγγράφου"@el .


###  http://okfn.gr/ontology/nsrf-gr#hasDecision
nsrf-gr:hasDecision rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain nsrf-gr:Expropriation ;
                    rdfs:range nsrf-gr:DecisionData ;
                    rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
                    rdfs:label "Has decision"@en ,
                               "Έχει απόφαση"@el .


###  http://okfn.gr/ontology/nsrf-gr#hasExpro
nsrf-gr:hasExpro rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain nsrf-gr:Project ;
                 rdfs:range nsrf-gr:Expropriation ;
                 rdfs:comment "The expropriation related to the project"@en ,
                              "Η απαλλοτρίωση που έχει το έργο"@el ;
                 rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
                 rdfs:label "Has expropriation"@en ,
                            "Έχει απαλλοτρίωση"@el .


###  http://okfn.gr/ontology/nsrf-gr#hasSubproject
nsrf-gr:hasSubproject rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf vfp:hasPart ,
                                         owl:topObjectProperty ;
                      owl:inverseOf nsrf-gr:subprojectOf ;
                      rdfs:domain nsrf-gr:Project ;
                      rdfs:range nsrf-gr:Subproject ;
                      rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
                      rdfs:label "has Subproject"@en ,
                                 "Έχει υποέργο"@el .


###  http://okfn.gr/ontology/nsrf-gr#implementor
nsrf-gr:implementor rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf vfp:organization ,
                                       owl:topObjectProperty ;
                    rdfs:domain nsrf-gr:Subproject ;
                    rdfs:range nsrf-gr:Body ;
                    rdfs:comment "Implementor of the subproject"@en ,
                                 "Ανάδοχος του υποέργου"@el ;
                    rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
                    rdfs:label "Implementor"@en ,
                               "Ανάδοχος"@el .


###  http://okfn.gr/ontology/nsrf-gr#location
nsrf-gr:location rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain vfp:Project ;
                 rdfs:range nsrf-gr:Area ;
                 rdfs:comment "Indicates the location of a project or support-grant in Greece"@en ,
                              "Τοποθεσία του έργου ή της ενίσχυσης στην ελληνική επικράτεια"@el ;
                 rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
                 rdfs:label "Location"@en ,
                            "Τοποθεσία"@el .


###  http://okfn.gr/ontology/nsrf-gr#operational
nsrf-gr:operational rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain vfp:Project ;
                    rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
                    rdfs:comment "The project is part of an operational programme"@en ,
                                 "Το έργο είναι μέρος ενός επιχειρησιακού προγράμματος"@el ;
                    rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
                    rdfs:label "Operational Programme"@en ,
                               "Επιχειρησιακό πρόγραμμα"@el .


###  http://okfn.gr/ontology/nsrf-gr#subprojectOf
nsrf-gr:subprojectOf rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf vfp:partOf ,
                                        owl:topObjectProperty ;
                     rdfs:domain nsrf-gr:Subproject ;
                     rdfs:range nsrf-gr:Project ;
                     rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
                     rdfs:label "subproject Of"@en ,
                                "Υποέργο του"@el .


###  http://okfn.gr/ontology/nsrf-gr#thematic
nsrf-gr:thematic rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain vfp:Project ;
                 rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
                 rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
                 rdfs:label "Thematic priority"@en ,
                            "Θεματική προτεραιότητα"@el .


###  http://okfn.gr/ontology/nsrf-gr#unitMeasure
nsrf-gr:unitMeasure rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain nsrf-gr:Expropriation ;
                    rdfs:isDefinedBy "http://okfn.gr/ontology/nsrf-gr" ;
                    rdfs:label "Unit of measure"@en ,
                               "Μονάδα μέτρησης"@el .


